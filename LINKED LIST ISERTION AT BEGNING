class llist {
    Node head;

    class Node {
        int data;
        Node next;

        Node(int val) {
            data = val;
            next = null;
        }
    }

  
    public void insert(int val) {
        Node newNode = new Node(val);
        newNode.next = head;
 
        head = newNode;
    }
    public void display() {
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.data + " -> ");
            temp = temp.next;
        }
    }
}

public class Main {
    public static void main(String[] args) {
        llist l = new llist();

        l.insert(100);
        l.insert(200);
        l.insert(300);
        l.insert(400);

        l.display();
    }
}
